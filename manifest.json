{
  "manifest_version": 2,
  "name": "ainsanity",
  "description": "Replace the terms Artificial Intelligence, AI etc. with Data-Driven Methods and DDM (and throws in a bar graph emoji)",
  "homepage_url": "http://www.memo.tv",
  "version": "1.1",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./emojiMap.js", "./substitute.js"]
    }
  ]
}
